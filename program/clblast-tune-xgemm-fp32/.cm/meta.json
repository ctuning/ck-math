    "CK_CLBLAST_NSIZE": 1024,
    "CK_COMPUTE_DEVICE_ID": 0,
    "CK_COMPUTE_PLATFORM_ID": 0,
    "CK_FORCE_RECOMPILE": 0
  },
  "tags": [
    "clblast",
    "cltune",
    "ck-autotune"
 ]
